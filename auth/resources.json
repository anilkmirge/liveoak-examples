{
  app: {
    type: 'filesystem',
    config: {
      directory: '${application.dir}/app'
    }
  },

  client: {
    type: 'aggregating-filesystem',
    config: {
      directory: "${io.liveoak.js.dir}"
    }
  },

  auth: {
    type: 'keycloak'
  },

  authz: {
    type: 'security'
  },

  uriPolicy: {
    type: 'uriPolicy'
  },

  storage: {
    type: 'mongo',
    config: {
      db: 'liveoak',
      servers: [
        {
          host: 'localhost',
          port: 27017
        }
      ]
    }
  }

}